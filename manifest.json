{
  "manifest_version": 3,

  "name": "Custom Youtube Plugin",
  "description": "Simple controls for looping, and jumping to most replayed if available",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*",
        "https://youtube.googleapis.com/*"
      ],
      "all_frames": true,
      "js": ["Custom_Youtube_Plugin.js"]
    }
  ]
}